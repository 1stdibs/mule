				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="locale" default="US" use="optional" type="LocaleEnumType">
					<xsd:annotation>
						<xsd:documentation>The locale to use for the format
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="pattern" use="optional" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>The pattern used to format the value
						</xsd:documentation>
