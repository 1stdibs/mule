	</xsd:element>

	<xsd:complexType name="ValidatePercentageType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="percentage" use="required" type="xsd:string">
