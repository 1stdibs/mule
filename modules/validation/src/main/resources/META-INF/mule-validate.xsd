	<xsd:element name="url" type="ValidateUrlType"
		xmlns="http://www.mulesoft.org/schema/mule/validate" xmlns:mule="http://www.mulesoft.org/schema/mule/core"
		substitutionGroup="validationMessageProcessor">
		<xsd:annotation>
			<xsd:documentation>If the specified url is
				not a valid one throw an
				exception.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateUrlType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="url" use="required" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>URL to validate</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="allow-two-slashes" default="false" use="optional" type="booleanType">
					<xsd:annotation>
						<xsd:documentation>Allow two slashes in the path component of the
							URL.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="allow-all-schemes" default="false" use="optional" type="booleanType">
					<xsd:annotation>
						<xsd:documentation>Allows all validly formatted schemes to pass
							validation instead of supplying a set of valid schemes.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="allow-local-urls" default="false" use="optional" type="booleanType">
					<xsd:annotation>
						<xsd:documentation>Allow local URLs, such as http://localhost/ or
							http://machine/ .
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="no-fragments" default="false" use="optional" type="booleanType">
					<xsd:annotation>
						<xsd:documentation>Enabling this options disallows any URL
							fragments.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

