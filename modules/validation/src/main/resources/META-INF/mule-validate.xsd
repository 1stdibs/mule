		xmlns:mule="http://www.mulesoft.org/schema/mule/core"
		substitutionGroup="validationMessageProcessor">
		<xsd:annotation>
			<xsd:documentation>If the given expression resolves to a true
				Boolean
				value throw an Exception
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="FalseType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="expression" use="required" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>A String literal or mule expression that can be
							resolves to a
							Boolean
						</xsd:documentation>
