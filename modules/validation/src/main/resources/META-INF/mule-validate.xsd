<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns="http://www.mulesoft.org/schema/mule/validate"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:mule="http://www.mulesoft.org/schema/mule/core"
	targetNamespace="http://www.mulesoft.org/schema/mule/validate"
	elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" />
	<xsd:import namespace="http://www.springframework.org/schema/beans"
		schemaLocation="http://www.springframework.org/schema/beans/spring-beans-3.1.xsd" />
	<xsd:import namespace="http://www.mulesoft.org/schema/mule/core"
		schemaLocation="http://www.mulesoft.org/schema/mule/core/3.4/mule.xsd" />


	<xsd:element name="validationMessageProcessor" abstract="true"
		substitutionGroup="mule:abstract-message-processor">
		<xsd:annotation>
			<xsd:documentation>
				A validating message processor
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="validationProcessorType">
		<xsd:complexContent>
			<xsd:extension base="mule:abstractMessageProcessorType"
				xmlns="http://www.mulesoft.org/schema/mule/validate">
				<xsd:attribute name="exception-class" type="mule:substitutableClass"
					use="optional">
					<xsd:annotation>
						<xsd:documentation>
							Allows customizing the class of the exception
							to
							be thrown if
							validation fails. Valid values are canonical names
							for
							classes that extend java.lang.Exception and have a constructor
							overriding java.lang.Exception(java.lang.String).
							If not provided,
							a org.mule.module.validation.ValidationException
							will be thrown in
							case of validation failure.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="message" type="xsd:string" use="optional">
					<xsd:annotation>
						<xsd:documentation>
							Allows customizing the message of the exception
							to be thrown if
							validation fails.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="domain" substitutionGroup="validationMessageProcessor"
		type="ValidateDomainType" xmlns="http://www.mulesoft.org/schema/mule/validate">
		<xsd:annotation>
			<xsd:documentation>If the specified domain does not parses as a valid
				domain name with a recognized top-level domain then throw an
				exception.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateDomainType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				base="validationProcessorType">
				<xsd:attribute name="domain" use="required" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Domain name to validate</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="top-level-domain" substitutionGroup="validationMessageProcessor"
		type="ValidateTopLevelDomainType" xmlns="http://www.mulesoft.org/schema/mule/validate"
		xmlns:mule="http://www.mulesoft.org/schema/mule/core">
		<xsd:annotation>
			<xsd:documentation>If if the specified
				top-level-domain does not
				matches any
				IANA-defined top-level domain throw an exception.
				Leading
				dots are
				ignored if present. The search is case-sensitive.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateTopLevelDomainType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="top-level-domain" use="required"
					type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Domain name to validate</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="top-level-domain-country"
		substitutionGroup="validationMessageProcessor" type="ValidateTopLevelDomainCountryType"
		xmlns="http://www.mulesoft.org/schema/mule/validate" xmlns:mule="http://www.mulesoft.org/schema/mule/core">
		<xsd:annotation>
			<xsd:documentation>If if the specified
				country-code does not matches
				any
				IANA-defined top-level domain throw an exception.
				Leading dots are
				ignored if present. The search is case-sensitive.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateTopLevelDomainCountryType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="country-code" use="required"
					type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Country code to validate</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element xmlns="http://www.mulesoft.org/schema/mule/validate"
		xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="credit-card-number"
		substitutionGroup="validationMessageProcessor" type="ValidateCreditCardNumberType">
		<xsd:annotation>
			<xsd:documentation>If the specified
				credit-card-number is not a valid
				credit card
				number throw an exception.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateCreditCardNumberType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="credit-card-number" use="required"
					type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Credit card number to validate
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="credit-card-type" use="required"
					type="CreditCardTypeEnumType">
					<xsd:annotation>
						<xsd:documentation>Credit card type to validate
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

