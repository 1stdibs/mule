				<xsd:attribute name="pattern" use="optional" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>The pattern used to format the value
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="using-regex" type="ValidateUsingRegexType"
		xmlns="http://www.mulesoft.org/schema/mule/validate" xmlns:mule="http://www.mulesoft.org/schema/mule/core"
		substitutionGroup="validationMessageProcessor">
		<xsd:annotation>
			<xsd:documentation>If the specified value
				does not match any of the
				regexs then throw an exception.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="ValidateUsingRegexType">
		<xsd:complexContent>
			<xsd:extension xmlns="http://www.mulesoft.org/schema/mule/validate"
				xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="validationProcessorType">
				<xsd:attribute name="value" use="required" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Value to match</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="regex" use="required" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Regular expression to test against
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute xmlns="http://www.mulesoft.org/schema/mule/validate"
					name="case-sensitive" default="false" use="optional" type="booleanType">
					<xsd:annotation>
						<xsd:documentation>when true matching is case sensitive, otherwise
							matching is case in-sensitive
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

