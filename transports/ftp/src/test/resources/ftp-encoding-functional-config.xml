<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:file="http://www.mulesource.org/schema/mule/file/2.2"
      xmlns:vm="http://www.mulesource.org/schema/mule/vm/2.2"
      xmlns:mule="http://www.mulesource.org/schema/mule/core/2.2"
      xmlns:ftp="http://www.mulesource.org/schema/mule/ftp/2.2"
      xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
       http://www.mulesource.org/schema/mule/file/2.2 http://www.mulesource.org/schema/mule/file/2.2/mule-file.xsd
       http://www.mulesource.org/schema/mule/ftp/2.2 http://www.mulesource.org/schema/mule/ftp/2.2/mule-ftp.xsd
       http://www.mulesource.org/schema/mule/vm/2.2 http://www.mulesource.org/schema/mule/vm/2.2/mule-vm.xsd">

    <vm:connector name="vmQueue" queueEvents="true"/>

    <ftp:connector name="ftpConnector" validateConnections="false"/>

    <endpoint address="vm://receive" name="receive"/>

    <model name="main">
        <service name="testInput">
            <inbound>
                <ftp:inbound-endpoint address="ftp://anonymous:password@localhost:60200"
                                      synchronous="true" binary="true" passive="true" connector-ref="ftpConnector"
                                      encoding="EUC-JP"/>
            </inbound>
            <outbound>
                <pass-through-router>
                    <outbound-endpoint ref="receive"/>
                </pass-through-router>
            </outbound>
        </service>
    </model>
</mule>