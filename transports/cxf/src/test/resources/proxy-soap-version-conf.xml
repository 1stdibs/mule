<?xml version="1.0" encoding="UTF-8"?>
<mule  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.mulesource.org/schema/mule/core/2.2"
       xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2"
       xmlns:cxf-core="http://cxf.apache.org/core"
       xmlns:vm="http://www.mulesource.org/schema/mule/vm/2.2"
       xsi:schemaLocation="
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2 http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd
               http://www.mulesource.org/schema/mule/vm/2.2 http://www.mulesource.org/schema/mule/vm/2.2/mule-vm.xsd
               http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd">

    <model>
        <service name="serverEcho">
            <inbound>
                <cxf:inbound-endpoint address="http://localhost:63081/services/Echo" proxy="true" soapVersion="1.2">
                    <cxf:features>
                        <cxf-core:logging/>
                    </cxf:features>
                </cxf:inbound-endpoint>
            </inbound>
            <outbound>
                <pass-through-router>
                    <vm:outbound-endpoint path="test" synchronous="true"/>
                </pass-through-router>
            </outbound>
        </service>

        <service name="echoService">
            <inbound>
                <inbound-endpoint address="vm://test" synchronous="true"/>
            </inbound>
            <echo-component/>
        </service>

        <service name="serverClientProxySoapVersion">
            <inbound>
                <cxf:inbound-endpoint address="http://localhost:63081/services/proxy-soap-version"
                                      proxy="true" payload="envelope" soapVersion="1.2">
                </cxf:inbound-endpoint>
            </inbound>
            <outbound>
                <pass-through-router>
                    <cxf:outbound-endpoint address="http://localhost:63081/services/Echo"
                        proxy="true"
                        payload="envelope"
                        synchronous="true"
                        soapVersion="1.2" />
                </pass-through-router>
            </outbound>
        </service>
    </model>
</mule>
