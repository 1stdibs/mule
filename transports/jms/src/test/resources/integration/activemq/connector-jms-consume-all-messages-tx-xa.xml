<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:jms="http://www.mulesoft.org/schema/mule/jms"
      xsi:schemaLocation="
       http://www.mulesoft.org/schema/mule/jms http://www.mulesoft.org/schema/mule/jms/3.1/mule-jms.xsd
       http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/3.1/mule.xsd">

    <jms:activemq-xa-connector name="jmsConnector" maxRedelivery="9999"/>
    <jms:activemq-connector name="noXaJmsConnector" maxRedelivery="9999"/>


    <jms:endpoint name="in" queue="in" connector-ref="noXaJmsConnector"/>
    <jms:endpoint name="out" queue="out" connector-ref="noXaJmsConnector"/>
    <jms:endpoint name="incoming" queue="in" connector-ref="jmsConnector"/>
    <jms:endpoint name="outgoing" queue="out" connector-ref="jmsConnector"/>

</mule>
